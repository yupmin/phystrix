{
    "name": "yupmin/modern-phystrix",
    "description": "Protocol agnostic library for latency and fault isolation",
    "license": "Apache-2.0",
    "keywords": [
        "odesk",
        "phystrix"
    ],
    "autoload": {
        "psr-0": {
            "Odesk\\": "library/"
        }
    },
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "psr/container": "^1.0",
        "zendframework/zend-config": "^3.2",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "php-di/php-di": "~5.4",
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
